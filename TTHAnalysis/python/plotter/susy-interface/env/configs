  alias       #:= {wBG : puw_nInt_Moriond(nTrueInt) * getLepSF(LepGood_pt[iLepSel[0] ], LepGood_eta[iLepSel[0] ], LepGood_pdgId[iLepSel[0] ]) * getLepSF(LepGood_pt[iLepSel[1] ], LepGood_eta[iLepSel[1] ], LepGood_pdgId[iLepSel[1] ]) * getLepSF(LepGood_pt[iLepSel[2] ], LepGood_eta[iLepSel[2] ], LepGood_pdgId[iLepSel[2] ]) * bTagWeight * triggerSFfullsim3L(LepGood_pt[iLepSel[0] ], LepGood_eta[iLepSel[0] ], LepGood_pt[iLepSel[1] ], LepGood_eta[iLepSel[1] ], LepGood_pt[iLepSel[2] ], LepGood_eta[iLepSel[2] ], met_pt, metmmm_pt(LepGood_pt[iLepSel[0] ], LepGood_phi[iLepSel[0] ], LepGood_pt[iLepSel[1] ], LepGood_phi[iLepSel[1] ], LepGood_pt[iLepSel[2] ], LepGood_phi[iLepSel[2] ], met_pt, met_phi, lepton_Id_selection(LepGood_pdgId[iLepSel[0] ], LepGood_pdgId[iLepSel[1] ], LepGood_pdgId[iLepSel[2] ])), lepton_permut( LepGood_pdgId[iLepSel[0] ], LepGood_pdgId[iLepSel[1] ], LepGood_pdgId[iLepSel[2] ]))}
  alias       #:= {wBGWZ : puw_nInt_Moriond(nTrueInt) * getLepSF(LepGood_pt[iLepSel[0] ], LepGood_eta[iLepSel[0] ], LepGood_pdgId[iLepSel[0] ]) * getLepSF(LepGood_pt[iLepSel[1] ], LepGood_eta[iLepSel[1] ], LepGood_pdgId[iLepSel[1] ]) * getLepSF(LepGood_pt[iLepSel[2] ], LepGood_eta[iLepSel[2] ], LepGood_pdgId[iLepSel[2] ]) * bTagWeight * triggerSFfullsimWZ(LepGood_pt[iLepSel[0] ], LepGood_eta[iLepSel[0] ], LepGood_pt[iLepSel[1] ], LepGood_eta[iLepSel[1] ], LepGood_pt[iLepSel[2] ], LepGood_eta[iLepSel[2] ], met_pt, metmmm_pt(LepGood_pt[iLepSel[0] ], LepGood_phi[iLepSel[0] ], LepGood_pt[iLepSel[1] ], LepGood_phi[iLepSel[1] ], LepGood_pt[iLepSel[2] ], LepGood_phi[iLepSel[2] ], met_pt, met_phi, lepton_Id_selection(LepGood_pdgId[iLepSel[0] ], LepGood_pdgId[iLepSel[1] ], LepGood_pdgId[iLepSel[2] ])), lepton_permut( LepGood_pdgId[iLepSel[0] ], LepGood_pdgId[iLepSel[1] ], LepGood_pdgId[iLepSel[2] ]))}
  alias       #:= {wFS : getLepSFFS(LepGood_pt[iLepSel[0] ], LepGood_eta[iLepSel[0] ], LepGood_pdgId[iLepSel[0] ]) * getLepSFFS(LepGood_pt[iLepSel[1] ], LepGood_eta[iLepSel[1] ], LepGood_pdgId[iLepSel[1] ]) * getLepSFFS(LepGood_pt[iLepSel[2] ], LepGood_eta[iLepSel[2] ], LepGood_pdgId[iLepSel[2] ]) * ISREwkCor * bTagWeightFS * triggerEff3L(LepGood_pt[iLepSel[0] ], LepGood_eta[iLepSel[0] ], LepGood_pt[iLepSel[1] ], LepGood_eta[iLepSel[1] ], LepGood_pt[iLepSel[2] ], LepGood_eta[iLepSel[2] ], met_pt, metmmm_pt(LepGood_pt[iLepSel[0] ], LepGood_phi[iLepSel[0] ], LepGood_pt[iLepSel[1] ], LepGood_phi[iLepSel[1] ], LepGood_pt[iLepSel[2] ], LepGood_phi[iLepSel[2] ], met_pt, met_phi, lepton_Id_selection(LepGood_pdgId[iLepSel[0] ], LepGood_pdgId[iLepSel[1] ], LepGood_pdgId[iLepSel[2] ])), lepton_permut( LepGood_pdgId[iLepSel[0] ], LepGood_pdgId[iLepSel[1] ], LepGood_pdgId[iLepSel[2] ])) }
  frFilesFS    := susy-sos-v1/3l/fakerate/fakerate_standard.txt;susy-sos-v1/3l/fakerate/fakerate_jec_central.txt;susy-sos-v1/3l/fakerate/fakerate_load_isprompt.txt
