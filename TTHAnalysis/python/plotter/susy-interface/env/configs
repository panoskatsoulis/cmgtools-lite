  wVarsFS     #:= {bTagFSHF : Sqrt(Power(bTagWeight_HF_Up/bTagWeight,2) + Power(bTagWeightFS_HF_Up/bTagWeightFS,2)) ; Sqrt(Power(bTagWeight_HF_Dn/bTagWeight,2) + Power( bTagWeightFS_HF_Dn/bTagWeightFS,2)) }
